{
  "name": "fetchling",
  "version": "1.0.0-alpha",
  "description": "Fetch portions (jp2 tiles) from a large remote sensing jpeg2000 image",
  "main": "dist/fetchling.js",
  "scripts": {
    "build": "rollup -c",
    "serve:example": "npm run build && cp ./dist/fetchling.js ./examples/js/fetchling.js && npx serve ./examples",
    "serve:test": "npx serve ./tests/data",
    "test": "node --experimental-modules ./tests/fetch.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvail/fetchling.git"
  },
  "keywords": [
    "j2k",
    "sentinel"
  ],
  "author": "jvail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jvail/fetchling/issues"
  },
  "homepage": "https://github.com/jvail/fetchling#readme",
  "devDependencies": {
    "node-fetch": "^2.6.1",
    "rollup": "^1.16.7",
    "serve": "^11.1.0",
    "terser": "^4.1.2"
  }
}
